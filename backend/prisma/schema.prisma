generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  shortCode   String   @unique
  originalUrl String   @db.Text
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
}
