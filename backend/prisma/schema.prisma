generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  shortCode   String   @unique @db.VarChar(20)
  originalUrl String   @db.VarChar(2048)
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
}
